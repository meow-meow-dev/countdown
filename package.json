{
  "name": "countdown",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "dev": "concurrently \"vite --config vite.config.server.ts\" \"vite --config vite.config.client.ts\"",
    "build": "vite build --config vite.config.client.ts && vite build --config vite.config.server.ts",
    "preview": "wrangler pages dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "tscheck": "tsc -noEmit -p tsconfig.app.json",
    "wrangler:types": "wrangler types --experimental-include-runtime=./src/wrangler.d.ts && rm -f worker-configuration.d.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.699.0",
    "@hono/valibot-validator": "^0.5.1",
    "@mantine/core": "^7.14.1",
    "@mantine/dates": "^7.14.1",
    "@mantine/form": "^7.14.1",
    "@mantine/hooks": "^7.14.1",
    "@mantine/modals": "^7.14.1",
    "@mantine/notifications": "^7.14.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "hono": "^4.6.12",
    "lucide-react": "^0.460.0",
    "luxon": "^3.5.0",
    "mantine-form-valibot-resolver": "^3.0.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.3.2",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "tailwind-merge": "^2.5.4",
    "valibot": "1.0.0-beta.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "github:antfu/eslint-config",
    "@eslint/js": "^9.13.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@hono/vite-dev-server": "^0.17.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "20.8.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.1",
    "tailwind-preset-mantine": "^1.3.1",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "wrangler": "^3.90.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
