{
  "name": "countdown",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#client/*": "./src/client/*/index.ts",
    "#client/locales/*": "./src/client/locales/*/messages.ts",
    "#server": "./src/server/index.ts"
  },
  "scripts": {
    "build": "pnpm generate_code && pnpm vite build --config vite.config.client.ts && pnpm vite build --config vite.config.server.ts",
    "dev": "pnpm concurrently \"vite --config vite.config.server.ts\" \"vite --config vite.config.client.ts\"",
    "generate_code": "pnpm wrangler:generate_types && pnpm lingui:extract && pnpm lingui:compile",
    "preview": "wrangler pages dev",
    "lingui:compile": "pnpm lingui compile --typescript",
    "lingui:extract": "pnpm lingui extract",
    "lint:check": "pnpm eslint .",
    "lint:fix": "pnpm eslint . --fix",
    "prepare": "pnpm husky",
    "prettier:check": "pnpm prettier --check .",
    "prettier:fix": "pnpm prettier --write .",
    "qa": "pnpm tsc:check && pnpm lint:check && pnpm prettier:check",
    "tsc:check": "pnpm tsc -noEmit --project tsconfig.app.json",
    "unit_tests": "pnpm vitest:client:run && pnpm vitest:server:run",
    "vitest:client:run": "pnpm vitest run --config=vite.config.client.ts --mode development --passWithNoTests",
    "vitest:client:watch": "pnpm vitest watch --config=vite.config.client.ts --mode development --passWithNoTests",
    "vitest:server:run": "pnpm vitest run --config=vitest.config.server.ts --mode development --passWithNoTests",
    "vitest:server:watch": "pnpm vitest watch --config=vitest.config.server.ts --mode development --passWithNoTests",
    "wrangler:generate_types": "pnpm wrangler types && mkdir -p ./src/server/_generated/wrangler && mv worker-configuration.d.ts ./src/server/_generated/wrangler"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.848.0",
    "@lingui/core": "^5.3.2",
    "@lingui/detect-locale": "^5.3.3",
    "@lingui/react": "^5.3.2",
    "@mantine/core": "^8.2.1",
    "@mantine/dates": "^8.2.1",
    "@mantine/form": "^8.2.1",
    "@mantine/hooks": "^8.2.1",
    "@mantine/modals": "^8.2.1",
    "@mantine/notifications": "^8.2.1",
    "@meow-meow-dev/server-utilities": "^4.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "hono": "^4.8.5",
    "lucide-react": "^0.525.0",
    "luxon": "^3.7.1",
    "mantine-form-standard-resolver": "^1.0.0",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.5",
    "@cloudflare/vitest-pool-workers": "0.8.8",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.31.0",
    "@hono/vite-build": "^1.6.2",
    "@hono/vite-dev-server": "^0.20.0",
    "@lingui/babel-plugin-lingui-macro": "^5.3.3",
    "@lingui/cli": "^5.3.3",
    "@lingui/vite-plugin": "^5.3.3",
    "@meow-meow-dev/shared-configs": "^1.7.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tanstack/eslint-plugin-router": "^1.129.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/luxon": "^3.6.2",
    "@types/node": "24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-macros": "^3.1.0",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "concurrently": "^9.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-package-json": "^0.42.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^7.5.3",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "oxlint": "^1.8.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-sql": "^0.19.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwind-preset-mantine": "^2.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.5",
    "vitest": "^3.1.3",
    "vitest-matchmedia-mock": "^2.0.3",
    "wrangler": "^4.25.0"
  },
  "packageManager": "pnpm@10.2.1",
  "engines": {
    "node": ">=21.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
